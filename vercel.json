{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/qr",
      "dest": "/api/qr.js"
    },
    {
      "src": "/code",
      "dest": "/api/pair.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
